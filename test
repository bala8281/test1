import json
import logging

def read_json_file(filename):
    try:
        with open(filename) as f:
            data = json.load(f)
            return data
    except Exception as e:
        logging.error('Failed to read JSON file: %s', filename)
        logging.exception(e)
        return None

def process_json_data(data):
    # Perform any processing on the JSON data
    # ...
    pass

def main():
    logging.basicConfig(filename='json_processing_log.txt', level=logging.INFO)

    filenames = ['file1.json', 'file2.json', 'file3.json', 'file4.json']

    for filename in filenames:
        data = read_json_file(filename)
        if data:
            process_json_data(data)

if __name__ == '__main__':
    main()
